    <script defer>
        $(window).on('scroll', function () {
                var scrollTop = $(window).scrollTop();
                if (scrollTop > 400) {
                    
                    $(".html").stop().animate({width: "75%"},1750);
                    $(".js").stop().animate({width: "40%"},1750);
                    $(".python").stop().animate({width: "35%"},1750);
                    $(".ofice").stop().animate({width: "60%"},1750);
                }
                else {
                    
                    $(".html").stop().animate({width: "1%"},750);
                    $(".js").stop().animate({width: "1%"},750);
                    $(".python").stop().animate({width: "1%"},750);
                    $(".ofice").stop().animate({width: "1%"},750);
                }
            });
    
    </script>
